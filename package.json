{
  "name": "sticker",
  "version": "1.1.5",
  "main": "lib/element-ui.common.js",
  "license": "MIT",
  "scripts": {
    "build:file": "node build/bin/build-entry.js",
    "build:css": "node build/bin/gen-cssfile && npx gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk",
    "watch:sass": "sass --watch packages/theme-chalk/src:test/css",
    "build": "npm run build:css && npm run build:file && webpack --config build/webpack.common.js && webpack --config build/webpack.component.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.demo.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config build/webpack.demo.js"
  },
  "devDependencies": {
    "cp-cli": "^2.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^2.1.0",
    "file-loader": "^1.1.11",
    "file-save": "^0.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "json-templater": "^1.2.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tslib": "^2.3.1",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.0.1",
    "vue": "2.5.21",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "2.5.21",
    "vue-template-es2015-compiler": "^1.6.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.11"
  }
}
