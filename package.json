    "node-sass": "^6.0.1",
    "tslib": "^2.3.1",
    "uppercamelcase": "^3.0.0",
    "vue": "2.5.21",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "2.5.21",
    "vue-template-es2015-compiler": "^1.6.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  }
